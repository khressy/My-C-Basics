#include <iostream>
using namespace std;

int main()
{
	
	int num;
	cout<<"enter number: ";
	cin>>num;
	
	for(int row=1; row<=num; row++)
	{
		for (int col=1; col<=num; col++)
		{
			if(row ==1 || row==num || col==1 || col==num)
			{
				cout<<"*";
			}
			else{
				cout<<" ";
			}
		}
		cout<<endl;
	}

return 0;
}

